{
    "name": "eaguad1337/nova-logs",
    "description": "Provides a Log Viewer for Laravel Nova",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Nyi Nyi Lwin",
            "email": "nyinyilwin1992@hotmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3|^8.0",
        "arcanedev/log-viewer": "^4.6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.6",
        "phpunit/phpunit": "7.1"
    },
    "autoload": {
        "psr-4": {
            "PhpJunior\\NovaLogViewer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpJunior\\NovaLogViewer\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PhpJunior\\NovaLogViewer\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
